{
    "name": "brunomaia/jqmphp",
    "description": "jqmPhp is a package of PHP classes that facilitates the creation HTML files for use with jQuery Mobile Framework. All classes in the jqmPhp package can be converted to string and printed with an echo function.",
    "authors": [
        {
            "name": "Bruno Maia",
            "email": "jqmphp@googlegroups.com"
        },
        {
            "name": "Menno Holtkamp"
        }
    ],
    "license": "GPL-3.0",
    "keywords": ["JQuery", "JQuery Mobile", "PHP"],
    "require": {
        "beberlei/assert": "~2.0",
        "php": ">=7.3"
    },
    "autoload": {
        "psr-4": {
            "jqmPhp\\": "lib/jqmPhp"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
        ,
        "squizlabs/php_codesniffer": "*"
    }
}
